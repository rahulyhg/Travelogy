    <link href="~/Content/css/magnific-popup.css" rel="stylesheet">
    <link href="~/Content/css/owl.carousel.css" rel="stylesheet">    
    <link href="~/Content/css/styles/skin-orange.css" rel="stylesheet" id="color_theme">
    <link href="~/Content/css/custom.css" rel="stylesheet">
    <link href="~/Content/assets/css/pages/page_log_reg_v1.css" rel="stylesheet">       
    <link href="~/Content/assets/css/pages/page_search.css" rel="stylesheet">    
    <link href="~/Content/css/styles/style.css" rel="stylesheet">
    <link href="~/Content/assets/css/style.css" rel="stylesheet">
    <link href="~/Content/assets/css/responsive.css" rel="stylesheet">
    <link href="~/Content/assets/css/pages/page_404_error.css" rel="stylesheet">
    <link href="~/Content/assets/plugins/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/assets/css/pages/portfolio-v2.css" rel="stylesheet" >    

    <!-- Favicon -->
    <link rel="shortcut icon" href="/">     
    <link rel="shortcut icon" href="favicon.ico">

    <!--Google analytics Tracking ID UA-84715163-1-->
    <script>
        (function (i, s, o, g, r, a, m) {
