@model IEnumerable<DomingoDAL.View_TripBookingTransport>

<div>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BookingDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransportFrom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransportTo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TravelClass)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BookingDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BookingStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransportFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransportTo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TravelClass)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.ActionLink("Manage", "EditTripBookingTransport", new { id = item.Id})                 
            </td>
        </tr>
    }
    
    </table>
</div>
</html>
