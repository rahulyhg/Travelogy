@model WebApplication1.Models.DomingoModelBase

@{
    if (Model != null && Model.Destination != null)
    {
        ViewBag.Description = Model.Destination.MetaDescription;
        ViewBag.KeyWords = Model.Destination.MetaKeywords;
    }
}

<!--=== Breadcrumbs ===-->
<div class="breadcrumbs">
    <div class="container">
        <ul class="pull-right breadcrumb">
            <li><a href="/India">India</a></li>
            <li><a href="/Circuit/India">All Circuits</a></li>
            <li class="active">@Model.Destination.Name</li>
        </ul>
    </div>
</div><!--/breadcrumbs-->
<!--=== End Breadcrumbs ===-->
@Html.Partial("_DestinationViewHeader", Model)

<div class="container">
    <div class="row">
        <div class="col-md-12 padd-10">
            <img class="img-responsive" src="~/Content/img/India/Wildlife01.jpg" />
        </div>
    </div>
</div>

<div class="block-heading-twelve text-center">
    <h3 class="orange">Coming soon</h3>
    <h2>Wildlife tours of India are not available yet, message us to register your interest or practical advice.</h2>
</div>

